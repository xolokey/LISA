{
  "name": "lisa---advanced-ai-personal-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "nodemon server/index.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node server/index.cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t lisa-ai .",
    "docker:run": "docker run -p 3000:3000 -p 5000:5000 lisa-ai"
  },
  "dependencies": {
    "@google/genai": "^1.20.0",
    "@prisma/client": "^6.16.2",
    "@tanstack/react-query": "^5.90.2",
    "@types/uuid": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "firebase": "^12.3.0",
    "framer-motion": "^12.23.19",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.16.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-button": "^0.8.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-jest": "^30.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-transform-stub": "^2.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "whatwg-fetch": "^3.6.20"
  }
}
